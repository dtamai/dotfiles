{
  "cargo.automaticCheck": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.columnSelection": false,
  "editor.fontFamily": "'Lilex', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.fontSize": 14,
  "editor.lineNumbers": "relative",
  "editor.tabSize": 2,
  "errorLens.followCursor": "closestProblem",
  "errorLens.fontSize": "14",
  "errorLens.fontStyleItalic": true,
  "errorLens.fontWeight": "200",
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "errorLens.statusBarIconsEnabled": true,
  "errorLens.statusBarIconsUseBackground": false,
  "errorLens.statusBarIconsAtZero": "removeBackground",
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    ".envrc": true,
    ".gem": true,
    "**/.direnv/**": true,
    "**/coverage": true,
    "**/node_modules/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "go.coverOnTestPackage": false,
  "go.enableCodeLens": {
    "references": false,
    "runtest": true
  },
  "go.formatTool": "goimports",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.extension.toc.levels": "2..6",
  "outline.showProperties": false,
  "outline.showVariables": false,
  "outline.showFunctions": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.updates.channel": "nightly",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.iconTheme": "sweet-vscode-icons",
  "workbench.colorTheme": "Eva Dark Italic",
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true
}
