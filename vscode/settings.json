{
  "cargo.automaticCheck": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.columnSelection": false,
  "editor.fontFamily": "'Lilex Medium', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.fontSize": 12,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.size": "fit",
  "editor.tabSize": 2,
  "editor.inlayHints.fontFamily": "Lilex Light",
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "errorLens.followCursor": "closestProblem",
  "errorLens.fontSize": "14",
  "errorLens.fontStyleItalic": true,
  "errorLens.fontWeight": "200",
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarIconsAtZero": "removeBackground",
  "errorLens.statusBarIconsEnabled": true,
  "errorLens.statusBarIconsUseBackground": false,
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    ".envrc": true,
    ".gem": true,
    "**/.direnv/**": true,
    "**/coverage": true,
    "**/node_modules/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "go.coverOnTestPackage": false,
  "go.enableCodeLens": {
    "references": false,
    "runtest": true
  },
  "go.formatTool": "goimports",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.extension.toc.levels": "2..6",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "shellcheck.customArgs": ["-x"],
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.iconTheme": "sweet-vscode-icons",
  "workbench.startupEditor": "none",
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
    }
  },
  "window.commandCenter": true,
  "window.titleBarStyle": "custom",
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.showTabs": false,
  "outline.showFunctions": false,
  "outline.showProperties": false,
  "outline.showVariables": false,
  "workbench.colorTheme": "Contrast"
}
